// ðŸ‘‰ filter() - print only filter element

let arr5 = [1,5,2,4,3]

const filter = arr5.filter(x => x < 3)

console.log(filter);

// ðŸ‘‰ moving zero to end 
arr=[1,2,3,9,0,0,0,8]

const filterr= arr.filter(x=>x!=0)

const filter2= arr.filter(x=> x==0)

console.log( finarr= filterr.concat(filter2))

//ðŸ‘‰ map method - returns an array
array= [1,2,5,7,8,30,6]
 const h= array.map(y => 
    
        (y+2)
        
    )
    console.log(h)
 

 //ðŸ‘‰ For each method - loops through the array elements , does not return an array 

let q=  array.forEach(element => {
    console.log(element)
 });
console.log(q) //returns undefined 

 //ðŸ‘‰sorting ascending 

 let r= array.sort((a,b)=>a-b)
 console.log(r) 
 
 // ðŸ‘‰ some() - check the item {atleast one} , return true either false
  console.log(array.some(x=> x==0))

  // ðŸ‘‰ Every() - check the item {all} , return true either false

  console.log(array.every(x=> x>0))

  // ðŸ‘‰ Find() - print only first element

  console.log(array.find(x=>x>2))

// ðŸ‘‰ Find() - last element which meets the condition

console.log(array.findLast(x=>x>2))

// ðŸ‘‰ Unshift() - add item(s) in first position, make changes to the actual array

let p=[1,5,3,9,8,4]
console.log(p.unshift(4,3)); //returns the size of array after changed
console.log(p);

//ðŸ‘‰ push()- Adds elements at the end of the arrays 

let push=[1,2,3,4,5]
push.push(9,8)
console.log(push)

//ðŸ‘‰  fill()- adds elements from the mentioned position till end
let fill=[ 4,6,7,3]

console.log(fill.fill(0,2))

// ðŸ‘‰ Shift() - remove the first element of array

let j=[1,2,3,4,5]
console.log(j.shift()); //returns how many elements it removed
console.log(j) //returns the array 

//ðŸ‘‰at()- reurns the array value at the specified index
const arr9= [1,2,3,4,89,45]
console.log(arr9.at(4))

//ðŸ‘‰ flat()
let arr13 = [1,5,2,[4],3,[5]]

const flat = arr9.flat()

console.log(flat)

//ðŸ‘‰ string method- from start index to end index

let qq5 = "Hello this is siya";

console.log(qq5);

let ee5 = qq5.slice(0,5)

console.log(ee5);



